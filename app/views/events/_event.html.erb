<div id="<%= dom_id event %>">
  <p>
    <strong>Category:</strong>
    <%= event.category %>
  </p>

  <p>
    <strong>Cover:</strong>
    <%= event.cover %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= event.date %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= event.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= event.end_time %>
  </p>

  <p>
    <strong>Indoors:</strong>
    <%= event.indoors %>
  </p>

  <p>
    <strong>Artist:</strong>
    <%= event.artist_id %>
  </p>

  <p>
    <strong>Venue:</strong>
    <%= event.venue_id %>
  </p>

</div>
